<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.mdPageController">
   <children>
      <ImageView fitHeight="100.0" fitWidth="1500.0" pickOnBounds="true" preserveRatio="true" />
      <HBox>
         <children>
            <ListView fx:id="fileList" prefHeight="200.0" prefWidth="200.0" />
             <AnchorPane fx:id="baseAnchorPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: white;">
               <children>
                  <ScrollPane fx:id="noteBlockScrollPane" fitToHeight="true" fitToWidth="true">
                     <content>
                        <VBox fx:id="blockDisplayVBox" prefHeight="900.0" prefWidth="1500.0" />
                     </content>
                  </ScrollPane>
               </children>
             </AnchorPane>
         </children>
      </HBox>
   </children>
</VBox>
